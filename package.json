{
  "name": "mygallery",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo build:web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "lint": "expo lint",
    "test": "expo test",
    "test:auth": "node scripts/test-auth-config.js",
    "get-redirect-uri": "node scripts/get-redirect-uri.js",
    "docker:build": "docker build -t mygallery-app:latest .",
    "docker:run": "docker run -d --name mygallery-container -p 8081:8081 -p 19000:19000 -p 19001:19001 -p 19002:19002 mygallery-app:latest",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:stop": "docker stop mygallery-container && docker rm mygallery-container",
    "docker:logs": "docker logs -f mygallery-container"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "expo": "~54.0.11",
    "expo-auth-session": "~7.0.8",
    "expo-av": "^16.0.7",
    "expo-constants": "^18.0.9",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.13",
    "expo-file-system": "^19.0.15",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "^8.0.8",
    "expo-media-library": "^18.2.0",
    "expo-permissions": "^14.4.0",
    "expo-sharing": "^14.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "~15.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@expo/config-plugins": "^54.0.2",
    "@expo/ngrok": "^4.1.3",
    "@types/react": "~19.1.0",
    "babel-plugin-dotenv-import": "^3.0.1",
    "babel-preset-expo": "^54.0.3",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
